{
  "manifest_version": 2,
  "name": "Custom add-on",
  "version": "1.4",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "description": "Does some handy stuff.",

  "content_scripts": [
    {
      "matches": ["https://create.kahoot.it/details/*", "https://create.kahoot.it/share/*"],
      "js": ["hide_kahoot_solutions.js"],
      "css": ["hide_kahoot_solutions.css"]
    },
    {
      "matches": ["*://*/*"],
      "js": ["remove_element.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "d9f97f24-7a88-4805-a2fa-4ff423bf587d@example.com"
    }
  },
  "background": {
    "scripts": ["modify-request.js"]
  },
  "web_accessible_resources": [
    "mock.json",
    "mock2.json"
  ]
}
